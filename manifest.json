{
  "manifest_version": 2,
  "name": "Blockr",
  "description": "A simple, lightweight post blocking solution for Tumblr",
  "version": "1.1.0",
  "author": { "name": "Elizabeth Harper", "email": "elliefops@gmail.com", "url": "https://github.com/EllieFops" },
  "incognito": "not_allowed",
  "background": { "scripts": [ "js/b.js" ], "persistent": false },
  "permissions": [ "tabs", "storage" ],
  "content_scripts": [
    { "matches": [ "https://www.tumblr.com/dashboard" ], "css": [ "css/a.css" ], "js": [ "js/a.js" ] }
  ],
  "icons": { "16": "res/16.png", "32": "res/32.png", "48": "res/48.png", "128": "res/128.png" },
  "page_action": { "default_icon": { "19": "res/19.png", "38": "res/38.png" }, "default_title": "Blockr" }
}
